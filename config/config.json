{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "quick-links-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/quickLinks/QuickLinksWebPart.js",
          "manifest": "./src/webparts/quickLinks/QuickLinksWebPart.manifest.json"
        }
      ]
    },
    "announcement-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/announcement/AnnouncementWebPart.js",
          "manifest": "./src/webparts/announcement/AnnouncementWebPart.manifest.json"
        }
      ]
    },
    "carousel-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/carousel/CarouselWebPart.js",
          "manifest": "./src/webparts/carousel/CarouselWebPart.manifest.json"
        }
      ]
    },
    "faq-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/faq/FaqWebPart.js",
          "manifest": "./src/webparts/faq/FaqWebPart.manifest.json"
        }
      ]
    },
    "shipments-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/shipments/ShipmentsWebPart.js",
          "manifest": "./src/webparts/shipments/ShipmentsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "QuickLinksWebPartStrings": "lib/webparts/quickLinks/loc/{locale}.js",
    "AnnouncementWebPartStrings": "lib/webparts/announcement/loc/{locale}.js",
    "CarouselWebPartStrings": "lib/webparts/carousel/loc/{locale}.js",
    "FaqWebPartStrings": "lib/webparts/faq/loc/{locale}.js",
    "ShipmentsWebPartStrings": "lib/webparts/shipments/loc/{locale}.js"
  }
}
